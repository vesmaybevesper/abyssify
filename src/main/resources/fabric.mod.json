{
	"schemaVersion": 1,
	"accessWidener": "vcc.accesswidener",
	"id": "vcc",
	"version": "${version}",
	"name": "Vesper's Compatibility Compendium",
	"description": "Various mod integrations, fixes and tweaks",
	"authors": [
		"VesMaybeVesper"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/vesmaybevesper/Vespers-Compatibility-Compendium"
	},
	"license": "LGPL-3.0",
	"icon": "assets/vcc/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"vesper.vcc.VCC"
		],
		"client": [
			"vesper.vcc.VCCClient"
		],
		"fabric-datagen": [
			"vesper.vcc.VCCDataGenerator"
		],
		"modmenu": [
			"vesper.vcc.ModMenuInit"
		]
	},
	"mixins": [
		"vcc.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
